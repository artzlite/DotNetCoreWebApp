@model PagingList<NetCoreWebAppBasic.DAL.TbMasterDistrict>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

Total Record Count: @Model.TotalRecordCount
<nav aria-label="Products navigation example">
    <vc:pager paging-list="@Model"></vc:pager>
</nav>

<table class="table">
    <tr>
        <th>
            @Html.SortableHeaderFor(model => model.DistrictNameThai)
        </th>
        <th>
            @Html.SortableHeaderFor(model => model.CreatedOn)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DistrictNameThai)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedOn)
            </td>
        </tr>
    }
</table>
<nav aria-label="Products navigation example">
    <vc:pager paging-list="@Model" />
</nav>

